{
  "name": "jobs",
  "version": "1.0.0",
  "description": "Job scheduling with Bree and node-cron",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
