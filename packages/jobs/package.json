    "data": "workspace:*",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
