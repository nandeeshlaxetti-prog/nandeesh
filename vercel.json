{
  "buildCommand": "cd apps/nandeesh-web && pnpm build",
  "outputDirectory": "apps/nandeesh-web/.next",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
