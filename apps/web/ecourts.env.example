# eCourts API Configuration
# Set the provider type: official, manual, or third_party
ECOURTS_PROVIDER=official

# API Key for official or third-party providers
# Get this from NAPIX, API Setu, or third-party services
ECOURTS_API_KEY=your_api_key_here

# Timeout for API requests (in milliseconds)
ECOURTS_TIMEOUT=30000

# Base URL override (optional)
# ECOURTS_BASE_URL=https://custom-api-endpoint.com

